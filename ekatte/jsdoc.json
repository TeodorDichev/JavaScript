{
    "source": {
        "include": ["search_ekatte\\src"], 
        "includePattern": ".+\\.js$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "template": "node_modules/docdash",
        "destination": "./docs/",
        "recurse": true
    },
    "docdash": {
        "search": true,
        "collapse": true,
        "typedefs": true
    }
}